// Online C compiler to run C program online
#include <stdio.h>
#define MAX_SIZE 100

int main()
{
    int arr[MAX_SIZE];
    int i, n, sum=0;

    printf("Enter size of the array: ");
    scanf("%d", &n);

    for(i=0; i<n; i++)
    {
         printf("Enter %d elements in the array: ", i);
        scanf("%d", &arr[i]);
    }

    for(i=0; i<n; i++)
    {
        sum = sum + arr[i];
    }

    printf("\nsum of all elements of array = %d", sum);

    return 0;
}        
