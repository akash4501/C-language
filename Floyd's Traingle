#include <stdio.h>

void floyd_traingle(int n){
    int count =1;
    for (int i =1;i<=n;i++){
        for (int j=1;j<=n;j++){
            if (i>=j){
                printf("%d",count);
                printf(" ");
                count ++;
            }
        }
        printf("\n");
    }
}    
int main() {
    int n;
    printf("Enter a number : ");
    scanf("%d",&n);
    floyd_traingle(n);
    return 0;
}
