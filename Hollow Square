#include <stdio.h>

void hollow_square(int n){
    for(int i=1; i<=n; i++){
        for(int j=1; j<=n ; j++){
            if((i==1) | (i==n) | (j==1) | (j==n)){
                printf("* ");
            }
            else{
                printf("  ");
            }
        }
        printf("\n");
    }
}

int main() {
    int n;
    printf("Enter a number : ");
    scanf("%d",&n);
    hollow_square(n);
    return 0;
} 
